{
  "global": {
    "scanUrls": [
      "org.openstreetmap.atlas"
    ],
    "filters": {
      "default": {
        "predicate": {
          "imports": [
            "org.openstreetmap.atlas.geography.atlas.items"
          ],
          "command": "e instanceof Area || e instanceof Relation"
        },
        "taggableFilter": "natural->water&water->reservoir|landuse->reservoir"
      }
    }
  }
}
