{
  "global": {
    "filters": {
      "default": {
        "predicate": {
          "imports": [
            "org.openstreetmap.atlas.geography.atlas.items"
          ],
          "command": "e instanceof Line || e instanceof Area || e instanceof Relation"
        },
        "taggableFilter": "natural->water&water->river|waterway->river,riverbank"
      }
    }
  }
}
